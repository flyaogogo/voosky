<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="web" extends="tmwrk-default" namespace="/web">
		<action name="getProInfoById" method="findProductInfoById" class="com.tmwrk.voosky.web.action.product.ProductAction">
			<result name="success">/public/product.jsp</result>
		</action>
	
		<action name="getArticleInfoById" method="findArticleInfoById" class="com.tmwrk.voosky.web.action.article.ArticleAction">
			<result name="success">/public/article.jsp</result>
		</action>
		
		<action name="getSPInfoById" method="findSinglePageInfoById" class="com.tmwrk.voosky.web.action.other.SinglePageAction">
			<result name="success">/public/page.jsp</result>
		</action>
		
		<action name="getProductsInfo" class="com.tmwrk.voosky.web.action.product.ProductAction">
			<result name="success">/public/productcategory.jsp</result>
		</action>
		
		<action name="getArticlesInfo" class="com.tmwrk.voosky.web.action.article.ArticleAction">
			<result name="success">/public/articlecategory.jsp</result>
		</action>
	</package>
</struts>